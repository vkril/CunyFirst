<style>


h1{
  font-size: 30px;
  color: #fff;
  text-transform: uppercase;
  font-weight: 300;
  text-align: center;
  margin-bottom: 10px;
}
table{
  width:70%;
  table-layout: fixed;
}
.tbl-header{
  background-color: rgba(255,255,255,.3);
 }
.tbl-content{
  height:auto;
  overflow-x:auto;
  margin-top: 0px;
  border: 5px solid rgba(255,255,255,0.3);
}
th{
  padding: 10px 10px;
  text-align: left;
  font-weight: 700;
  font-size: 16px;
  color: #fff;
  text-transform: uppercase;
}
td{
  padding: 15px;
  text-align: left;
  vertical-align:middle;
  font-weight: 600;
  font-size: 15px;
  color: #fff;
  border-bottom: solid 1px rgba(255,255,255,0.1);
}


/* demo styles */

@import url(https://fonts.googleapis.com/css?family=Roboto:400,500,300,700);
body{
  background: -webkit-linear-gradient(left, #25c481, #25b7c4);
  background: linear-gradient(to right, #253481, #2557c4);
  font-family: 'Roboto', sans-serif;
}
section{
  margin: 50px;
}





/* for custom scrollbar for webkit browser*/

::-webkit-scrollbar {
    width: 6px;
}
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
}
::-webkit-scrollbar-thumb {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
}
</style>




<section>
  <!--for demo wrap-->
  <h1>Search Result</h1>
  <div class="tbl-header">
    <table cellpadding="0" cellspacing="0" border="0">
      <thead>
        <tr>
          <th>Name</th>
          <th>Description</th>
          <th>Day</th>
          <th>Room</th>
          <th>Start Time</th>
          <th>Finish Time</th>
          <th>Credits</th>
          <th>Term</th>
          <th>First_Name</th>
          <th>Last_Name</th>
        </tr>
      </thead>
    </table>
  </div>
  <div class="tbl-content">
    <table cellpadding="0" cellspacing="0" border="0">
      <tbody>
        {{#each results}}
          <tr>
            <td>{{Name}} </td>
            <td>{{Description}}</td>
            <td>{{Day}}</td>
            <td>{{Room}}</td>
            <td>{{Start_Time}}</td>
            <td>{{Finish_Time}}</td>
            <td>{{Credits}}</td>
            <td>{{Term}}</td>
            <td>{{First_Name}}</td>
            <td>{{Last_Name}}</td>
          </tr>

        {{/each}}

      </tbody>
    </table>
  </div>
</section>
